{
  "name": "telegram-bot-js",
  "version": "1.0.0",
  "description": "Build telegram bot using javascript",
  "main": "./app/index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start:docker": "docker compose up -d",
    "stop:docker": "docker compose down",
    "clear:docker": "docker container prune",
    "dev": "yarn stop:docker && sleep 1 && yarn start:docker && sleep 1 && nodemon ./app/index.js",
    "start": "node ./app/index.js"
  },
  "dependencies": {
    "compression": "1.8.0",
    "connect-redis": "8.1.0",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "5.1.0",
    "express-rate-limit": "7.5.0",
    "express-session": "1.18.1",
    "helmet": "8.1.0",
    "ioredis": "5.6.1",
    "jsonwebtoken": "9.0.2",
    "mongoose": "8.14.1",
    "morgan": "1.10.0",
    "node-telegram-bot-api": "0.66.0",
    "radash": "12.1.0",
    "valibot": "1.1.0",
    "xss": "1.0.15"
  },
  "devDependencies": {
    "nodemon": "3.1.10"
  }
}
